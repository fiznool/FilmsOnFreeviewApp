{
  "parser": "babel-eslint",
  "plugins": ["react", "react-native"],
  "globals": {
    "__DEV__": true,
    "require": true,
    "console": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-native/all"
  ],
  "rules": {
    "react/display-name": 0,
    "quotes": [2, "single", { "avoidEscape": true }]
  }
}
